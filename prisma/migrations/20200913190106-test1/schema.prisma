// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema


model Movie {
  id           Int    @default(autoincrement()) @id
  director     String
  movieName    String
  yearReleased Int
}

model User {
  id           Int    @default(autoincrement()) @id
  email     String
  name      String
  mobile    String
  password  String
}


datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}



